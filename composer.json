{
    "name": "lawoole/framework",
    "description": "Lawoole is a high-performance PHP framework based on Laravel and Swoole",
    "keywords": ["framework", "swoole", "laravel", "lawoole"],
    "license": "MIT",
    "homepage": "https://github.com/lawoole/framework",
    "support": {
        "issues": "https://github.com/lawoole/framework/issues",
        "source": "https://github.com/lawoole/framework"
    },
    "authors": [
        {
            "name": "You Ming",
            "email": "youming@funcuter.org"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "5.7.*"
    },
    "require-dev": {
        "lawoole/swoole": "dev-master",
        "phpunit/phpunit": "~7.0",
        "guzzlehttp/guzzle": "~6.0",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Lawoole\\": "src/"
        },
        "files": [
            "src/Foundation/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
