{
    "name": "lawoole/framework",
    "description": "A fast framework with Laravel components based on Swoole.",
    "keywords": ["framework", "swoole", "laravel", "lawoole"],
    "license": "MIT",
    "homepage": "https://github.com/lawoole/framework",
    "support": {
        "issues": "https://github.com/lawoole/framework/issues",
        "source": "https://github.com/lawoole/framework"
    },
    "authors": [
        {
            "name": "You Ming",
            "email": "youming@funcuter.org"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "illuminate/cache": "5.6.*",
        "illuminate/config": "5.6.*",
        "illuminate/console": "5.6.*",
        "illuminate/container": "5.6.*",
        "illuminate/contracts": "5.6.*",
        "illuminate/database": "5.6.*",
        "illuminate/events": "5.6.*",
        "illuminate/filesystem": "5.6.*",
        "illuminate/http": "5.6.*",
        "illuminate/log": "5.6.*",
        "illuminate/pipeline": "5.6.*",
        "illuminate/queue": "5.6.*",
        "illuminate/routing": "5.6.*",
        "illuminate/support": "5.6.*",
        "dragonmantank/cron-expression": "~2.0",
        "ramsey/uuid": "^3.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "lawoole/swoole": "dev-master",
        "illuminate/auth": "5.6.*",
        "illuminate/encryption": "5.6.*",
        "illuminate/validation": "5.6.*",
        "illuminate/view": "5.6.*"
    },
    "autoload": {
        "psr-4": {
            "Lawoole\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
